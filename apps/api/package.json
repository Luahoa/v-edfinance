  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:fast": "vitest run --exclude **/nudge-scheduler.service.spec.ts --exclude **/ai.integration.spec.ts --exclude **/moderation.service.spec.ts --exclude **/ai-cache.service.spec.ts --exclude **/analytics.service.spec.ts --exclude **/heatmap.service.spec.ts --exclude **/metrics.service.spec.ts --exclude **/reports.service.spec.ts --exclude **/user-segmentation.service.spec.ts --exclude **/realtime-dashboard.service.spec.ts --exclude **/ab-testing.service.spec.ts",
    "test:ava": "ava \"src/**/*.ava.ts\" --no-cache",
    "test:ava:file": "ava",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/vitest --runInBand",
    "test:e2e": "vitest run --config ./test/vitest-e2e.config.ts",
    "test:db:start": "docker-compose -f ../../docker-compose.test.yml up -d postgres-test",
    "test:db:stop": "docker-compose -f ../../docker-compose.test.yml down",
    "test:db:reset": "prisma migrate reset --force --skip-seed",
    "db:generate": "prisma generate",
    "db:erd": "prisma generate && echo ERD generated at docs/erd.md",
    "db:seed": "ts-node prisma/seeds/index.ts",
    "db:seed:dev": "ts-node prisma/seeds/scenarios/dev.seed.ts",
    "db:seed:test": "ts-node prisma/seeds/scenarios/test.seed.ts",
    "db:seed:demo": "ts-node prisma/seeds/scenarios/demo.seed.ts",
    "db:seed:benchmark": "ts-node prisma/seeds/scenarios/benchmark.seed.ts",
    "db:nocodb:up": "docker-compose -f ../../docker-compose.nocodb.yml up -d",
    "db:nocodb:down": "docker-compose -f ../../docker-compose.nocodb.yml down",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:studio": "drizzle-kit studio",
    "ai:optimize": "tsx src/modules/ai/scripts/optimize-database.ts",
    "ai:optimize:prod": "node dist/modules/ai/scripts/optimize-database.js"
  },
