{
  "name": "@v-edfinance/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/client.ts --format esm --no-dts",
    "dev": "tsup src/index.ts src/client.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trpc/client": "^11.0.0-rc.666",
    "@trpc/react-query": "^11.0.0-rc.666",
    "@trpc/server": "^11.0.0-rc.666",
    "@tanstack/react-query": "^5.62.16",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
